# Production Environment Configuration
NODE_ENV=production
VITE_NODE_ENV=production

# Supabase Production Instance
VITE_SUPABASE_URL=https://your-prod-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-prod-anon-key

# Database
VITE_DATABASE_URL=postgresql://username:password@prod-host:5432/subtracker_prod

# API Configuration
VITE_API_BASE_URL=https://api.subtracker.com/api
VITE_APP_URL=https://app.subtracker.com

# Feature Flags (Production)
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_TELEMETRY=true
VITE_ENABLE_DEBUG=false
VITE_ENABLE_DEV_TOOLS=false

# External Services (Production)
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_production_key
VITE_GOOGLE_CLIENT_ID=production_google_client_id
VITE_MICROSOFT_CLIENT_ID=production_microsoft_client_id

# Logging
VITE_LOG_LEVEL=error
VITE_ENABLE_CONSOLE_LOGS=false

# Testing
VITE_TEST_MODE=false
VITE_MOCK_API=false

# Performance
VITE_ENABLE_SOURCE_MAPS=false
VITE_ENABLE_HOT_RELOAD=false

# Security (Strict for production)
VITE_ENABLE_HTTPS=true
VITE_COOKIE_SECURE=true
VITE_CSRF_PROTECTION=true

# CDN and Assets
VITE_CDN_URL=https://cdn.subtracker.com
VITE_ASSETS_BASE_URL=https://assets.subtracker.com

# Error Tracking
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
VITE_ENABLE_ERROR_REPORTING=true
