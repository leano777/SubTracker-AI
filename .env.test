# Testing Environment Configuration
NODE_ENV=test
VITE_NODE_ENV=test

# Test Database
VITE_SUPABASE_URL=https://test-project.supabase.co
VITE_SUPABASE_ANON_KEY=test-anon-key
VITE_DATABASE_URL=postgresql://username:password@localhost:5432/subtracker_test

# API Configuration (Mock/Local)
VITE_API_BASE_URL=http://localhost:3001/api
VITE_APP_URL=http://localhost:3000

# Feature Flags (Testing)
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_TELEMETRY=false
VITE_ENABLE_DEBUG=true
VITE_ENABLE_DEV_TOOLS=true

# External Services (Mock)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_mock_key
VITE_GOOGLE_CLIENT_ID=mock_google_client_id
VITE_MICROSOFT_CLIENT_ID=mock_microsoft_client_id

# Logging
VITE_LOG_LEVEL=warn
VITE_ENABLE_CONSOLE_LOGS=false

# Testing Specific
VITE_TEST_MODE=true
VITE_MOCK_API=true
VITE_MOCK_DATA=true
VITE_SKIP_AUTH=true
VITE_MOCK_TIMERS=true

# Performance (Fast for tests)
VITE_ENABLE_SOURCE_MAPS=false
VITE_ENABLE_HOT_RELOAD=false
VITE_PARALLEL_TESTS=true

# Security (Minimal for testing)
VITE_ENABLE_HTTPS=false
VITE_COOKIE_SECURE=false
VITE_CSRF_PROTECTION=false
