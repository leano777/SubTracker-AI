# Development Environment Configuration
NODE_ENV=development
VITE_NODE_ENV=development

# Supabase Development Instance
VITE_SUPABASE_URL=https://your-dev-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-dev-anon-key

# Database
VITE_DATABASE_URL=postgresql://username:password@localhost:5432/subtracker_dev

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_APP_URL=http://localhost:5173

# Feature Flags (Development)
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_TELEMETRY=false
VITE_ENABLE_DEBUG=true
VITE_ENABLE_DEV_TOOLS=true

# External Services (Development/Mock)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_development_key
VITE_GOOGLE_CLIENT_ID=development_google_client_id
VITE_MICROSOFT_CLIENT_ID=development_microsoft_client_id

# Logging
VITE_LOG_LEVEL=debug
VITE_ENABLE_CONSOLE_LOGS=true

# Testing
VITE_TEST_MODE=true
VITE_MOCK_API=true

# Performance
VITE_ENABLE_SOURCE_MAPS=true
VITE_ENABLE_HOT_RELOAD=true

# Security (Relaxed for development)
VITE_ENABLE_HTTPS=false
VITE_COOKIE_SECURE=false
VITE_CSRF_PROTECTION=false
