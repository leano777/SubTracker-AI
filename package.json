{
  "name": "subtracker-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 5175",
    "build": "tsc -b && vite build",
    "build:deploy": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview --host --port 5175",
    "a11y:audit": "node scripts/accessibility-audit.js",
    "a11y:test": "npm run test -- src/test/accessibility",
    "a11y:full": "npm run a11y:test && npm run a11y:audit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run src/test/hooks src/test/contexts",
    "test:integration": "vitest run src/test/integration",
    "test:performance": "vitest run src/test/performance",
    "test:ci": "vitest run --coverage --reporter=verbose --reporter=junit --outputFile.junit=./test-results/junit.xml",
    "test:profile": "vitest run --reporter=verbose src/test/performance",
    "type-check": "tsc --noEmit",
    "quality:check": "npm run type-check && npm run lint && npm run test:run",
    "ci:test": "npm run quality:check && npm run test:ci",
    "precommit": "npm run format && npm run lint:fix && npm run test:unit",
    "dev:clean": "rimraf dist && npm run dev",
    "dev:test": "cross-env NODE_ENV=development npm run test:watch",
    "dev:debug": "cross-env NODE_ENV=development vite --debug",
    "test:env": "cross-env NODE_ENV=test npm run test:run",
    "test:integration:env": "cross-env NODE_ENV=test npm run test:integration",
    "test:e2e:vitest": "cross-env NODE_ENV=test vitest run src/test/e2e",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "build:staging": "cross-env NODE_ENV=staging npm run build",
    "preview:prod": "cross-env NODE_ENV=production npm run preview",
    "preview:staging": "cross-env NODE_ENV=staging npm run preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "test:visual": "playwright test --config=playwright.config.ts",
    "test:visual:headed": "playwright test --config=playwright.config.ts --headed",
    "test:visual:debug": "playwright test --config=playwright.config.ts --debug",
    "test:visual:update": "playwright test --config=playwright.config.ts --update-snapshots",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:headed": "cypress run --headed",
    "test:visual:ci": "playwright test --config=playwright.config.ts --reporter=junit",
    "test:all-visual": "npm run test:visual && npm run test:e2e",
    "visual:setup": "playwright install",
    "visual:report": "playwright show-report",
    "verify:visual-setup": "node scripts/verify-visual-setup.js",
    "test:smoke": "node scripts/run-smoke-tests.js",
    "test:smoke:ci": "cypress run --spec \"tests/e2e/smoke-tests/ST-003-smoke-tests.cy.ts\" --reporter junit --reporter-options \"mochaFile=test-results/smoke/ST-003-junit.xml\"",
    "test:smoke:headed": "cypress run --spec \"tests/e2e/smoke-tests/ST-003-smoke-tests.cy.ts\" --headed",
    "smoke:report": "open test-results/reports/ST-003-smoke-report.html"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@sentry/react": "^7.91.0",
    "@sentry/tracing": "^7.91.0",
    "@supabase/supabase-js": "^2.54.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.12",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.468.0",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.3",
    "react": "^19.1.1",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-is": "^19.1.1",
    "react-resizable-panels": "^3.0.4",
    "react-router-dom": "^6.21.0",
    "recharts": "^3.1.2",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.17",
    "vaul": "^1.1.2",
    "zod": "^4.0.15",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.10.2",
    "@axe-core/puppeteer": "^4.10.2",
    "@axe-core/react": "^4.10.2",
    "@chromatic-com/storybook": "^4.1.0",
    "@eslint/js": "^9.32.0",
    "@playwright/experimental-ct-react": "^1.54.2",
    "@playwright/test": "^1.54.2",
    "@radix-ui/react-label": "^2.1.7",
    "@storybook/addon-a11y": "^9.1.1",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-backgrounds": "^9.0.8",
    "@storybook/addon-controls": "^9.0.8",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-onboarding": "^9.1.1",
    "@storybook/addon-toolbars": "^9.0.8",
    "@storybook/addon-viewport": "^9.0.8",
    "@storybook/react-vite": "^9.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^2.1.8",
    "axe-core": "^4.10.3",
    "chromatic": "^13.1.3",
    "cross-env": "^10.0.0",
    "cypress": "^14.5.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.1",
    "globals": "^16.3.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.0.0",
    "pixelmatch": "^7.1.0",
    "playwright": "^1.54.2",
    "prettier": "^3.4.2",
    "puppeteer": "^24.16.0",
    "rimraf": "^6.0.1",
    "storybook": "^9.1.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0",
    "vitest": "^2.1.8"
  }
}
